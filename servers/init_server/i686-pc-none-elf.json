{
  "arch": "x86",
  "cpu": "pentium4",
  "llvm-target": "i686-pc-none-elf",
  "target-endian": "little",
  "target-pointer-width": "32",
  "os": "none",
  "data-layout": "e-m:e-p:32:32-p270:32:32-p271:32:32-p272:64:64-i128:128-f64:32:64-f80:32-n8:16:32-S128",
  "pre-link-args": {
    "gcc": [
      "-ffreestanding",
      "-nostdlib",
      "-Wl,--script=link.ld",
      "-g",
      "-ggdb"
    ]
  },

  "features": "-sse3,-ssse3,-sse4.1,-sse4.2,-avx,-avx2,-soft-float",
  "disable-redzone": true,
  "linker-is-gnu": true,
  "archive-format": "gnu",
  "executables": true,
  "linker": "i686-elf-gcc",
  "eh-frame-header": false,
  "exe-suffix": ".exe"
}
